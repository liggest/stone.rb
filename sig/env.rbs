
module Stone
  interface _Env
    def set: (String name, Object val) -> void
    def get: (String name) -> Object
  end
  
  class BaseEnv
    include _Env

    attr_reader vals: Hash[String,Object]

    def initialize: () -> void
                  
    def fetch: ?{ -> Object } -> Object
                  
    alias [] get
    alias []= set

  end
end
